<h1>Statistics#show</h1>
<%- model_class = Result -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
  <%- model_class = Result -%>
  <div class="panel panel-default">
    <div class="panel-body">
      <div class="row">
        <div class="col-md-1"><%= model_class.human_attribute_name(:id) %>:</div>
        <div class="col-md-4"><%= @result.id%></div>
      </div>
      <div class="row">
        <div class="col-md-1"><%= model_class.human_attribute_name(:title) %>:</div>
        <div class="col-md-4"><%= @result.title%></div>
      </div>
      <div class="row">
        <div class="col-md-1"><%= model_class.human_attribute_name(:link) %>:</div>
        <div class="col-md-4"><%= @result.link%></div>
      </div>
      <%= render partial:'statistics/partial/entry', collection: @result.latest_per_query%>
      </div>
    </div>
  </div>
</div>




